---
import {Header} from '../components/Header'
import MainHead from './MainHead.astro';

interface Props {
  title: string
  description: string
}
const {title, description} = Astro.props as Props
---

<html lang="en">
	<MainHead title={title} description={description}/>
	<body>
		<Header/>
    <slot>Default</slot>
	</body>
</html>
 